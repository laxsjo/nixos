        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "p:textLight",
                    "template": "╭─"
                },
                {
                    "type": "os",
                    "style": "diamond",
                    "leading_diamond": "",
                    "powerline_symbol": "",
                    "foreground": "p:text",
                    "background": "p:blue",
                    "template": "♥ ʕ•ᴥ•ʔっ{{.Icon}} ",
                    "background_templates": [
                        "{{if eq .OS \"debian\"}}p:sapphire{{end}}"
                    ]
                },
                {
                    "type": "root",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "foreground": "p:text",
                    "background": "p:red",
                    "template": "<parentBackground></>  "
                },
                {
                    "type": "path",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "foreground": "p:text",
                    "background": "p:surface",
                    "template": " {{ .Parent }}{{ if or .Parent .RootDir }}<p:blue>{{ .Path }}</>{{ else }}{{ .Path }}{{ end }} ",
                    "properties": {
                        "style": "folder",
                        "home_icon": "<p:green>~</>"
                    }
                }
            ]
        },
